   "source": [
    "def ex_gcd(a, b):\n",
    "    if b == 0:\n",
    "        return 1, 0, a\n",
    "    x = ex_gcd(b, a%b)[0]\n",
    "    y = ex_gcd(b, a%b)[1]\n",
    "    d = ex_gcd(b, a % b)[2]\n",
    "    return y ,x-(a//b)*y, d\n",
    "\n",
    "\n",
    "a = int(input())\n",
    "b = int(input())\n",
    "ex_gcd(max(a, b), min(a, b))"
   ]
