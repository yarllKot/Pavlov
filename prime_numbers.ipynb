   "source": [
    "import math\n",
    "\n",
    "\n",
    "def check(a):\n",
    "    if (a<=1): return False\n",
    "    for i in range(2, int(math.sqrt(a))+1):\n",
    "        if a%i == 0:\n",
    "            return False\n",
    "    return True\n",
    "\n",
    "\n",
    "a = int(input())\n",
    "if check(a):\n",
    "    print(\"It's a prime number\")\n",
    "else: print(\"It's not a prime number\")\n",
    "\n"
   ]
